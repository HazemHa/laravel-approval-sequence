{
  "name": "hazemha/laravel-approval-sequence",
  "description": "Attach approvals to any model to prevent unauthorised updates.",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "name": "Hazem Hatem",
    "email": "hazemhahussain@gmail.com"
  }],
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "laravel/framework": "~5.6.0|~5.7.0|~5.8.0",
    "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0"
  },
  "autoload": {
    "psr-4": {
      "ApprovalSequence\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApprovalSequence\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ApprovalSequence\\ApprovalSequenceServiceProvider"
      ]
    }
  }
}
